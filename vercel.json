{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/index.go": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "env": {
    "VERCEL": "1"
  }
}
